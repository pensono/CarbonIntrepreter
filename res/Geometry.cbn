// We're ignoring the fact that Width Height and Area all have different units

Point = {
    PosX,
    PosY,
};

NamedPoint = Point : {
    Name:String,
};

Point(Mag:Unit, Angle) = Point(cos(Angle)*Mag, sin(Angle)*Mag);

